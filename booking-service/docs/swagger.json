        "/bookings/{booking_id}": {
            "get": {
                "description": "Get detailed information about a specific booking\nGet detailed information about a specific booking",
                "produces": [
                    "application/json",
                    "application/json"
                ],
                "tags": [
                    "bookings",
                    "bookings"
                ],
                "summary": "Get booking information",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Booking ID",
                        "name": "booking_id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Booking ID",
                        "name": "booking_id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Booking information retrieved successfully",
                        "schema": {
                            "$ref": "#/definitions/domain.Booking"
                        }
                    }
                }
            }
        },
        "/showtimes": {
            "get": {
                "description": "Get a list of all available showtimes",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "showtimes"
